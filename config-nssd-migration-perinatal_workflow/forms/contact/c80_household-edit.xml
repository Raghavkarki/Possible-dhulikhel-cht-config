      <instance>
        <data id="c80_household-edit" prefix="J1!c80_household-edit!" delimiter="#" version="1">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <c80_household>
            <parent/>
            <type/>
            <contact_type/>
            <hhID/>
            <name/>
            <generated_note_name_15/>
            <household_type/>
            <house_no_suffix/>
            <house_no/>
            <parent_place_code/>
            <street_name/>
            <hh_no_prefix_edited/>
            <hhID_edited/>
            <today/>
            <primary_purpose/>
            <purpose_specify/>
            <house_owner>
              <house_owner_given_name/>
              <house_owner_middle_name/>
              <house_owner_family_name/>
            </house_owner>
            <house_owner_house_name/>
            <family_units/>
            <ask_chhaupadi/>
            <stay_menstruate/>
            <woman_stay_menstruate/>
            <observation_shed/>
            <survey_complete/>
            <survey_incomplete_note/>
            <survey_complete_note/>
            <reporting_today/>
            <reporting_report_survey_complete/>
          </c80_household>
          <c80_household_count/>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs/user" relevant="0"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/c80_household_count" type="string" readonly="true()" calculate="जरुरी"/>
      <bind nodeset="/data/c80_household/parent" type="string" required="true()"/>
      <bind nodeset="/data/c80_household/type" type="string" required="true()"/>
      <bind nodeset="/data/c80_household/contact_type" type="string" required="true()"/>
      <bind nodeset="/data/c80_household/hhID" type="string" calculate=" /data/c80_household/hhID_edited "/>
      <bind nodeset="/data/c80_household/name" type="string" calculate="concat( /data/c80_household/hhID_edited , ' - ', concat( /data/c80_household/house_owner/house_owner_given_name , &quot; &quot;,  /data/c80_household/house_owner/house_owner_middle_name , &quot; &quot;,  /data/c80_household/house_owner/house_owner_family_name ))"/>
      <bind nodeset="/data/c80_household/generated_note_name_15" readonly="true()" type="string"/>
      <bind nodeset="/data/c80_household/household_type" type="string"/>
      <bind nodeset="/data/c80_household/house_no_suffix" type="string" relevant=" /data/c80_household/household_type  = 'metric_system'"/>
      <bind nodeset="/data/c80_household/house_no" type="string" relevant=" /data/c80_household/household_type  = 'non_metric_system'"/>
      <bind nodeset="/data/c80_household/parent_place_code" type="string"/>
      <bind nodeset="/data/c80_household/street_name" type="string"/>
      <bind nodeset="/data/c80_household/hh_no_prefix_edited" type="string" calculate="if( /data/c80_household/house_no  &lt; 10, &quot;000&quot;, if( /data/c80_household/house_no  &lt; 100, &quot;00&quot;, if( /data/c80_household/house_no  &lt; 1000, &quot;0&quot;, &quot;&quot;)))"/>
      <bind nodeset="/data/c80_household/hhID_edited" type="string" calculate="if( /data/c80_household/household_type  = 'non_metric_system', concat( /data/c80_household/parent_place_code ,  /data/c80_household/hh_no_prefix_edited ,  /data/c80_household/house_no ), concat( /data/c80_household/parent_place_code , &quot;-&quot;,  /data/c80_household/street_name , &quot;-&quot;,  /data/c80_household/house_no_suffix ))"/>
      <bind nodeset="/data/c80_household/today" type="string" calculate="format-date(date(today()), &quot;%d-%m-%Y&quot;)"/>
      <bind nodeset="/data/c80_household/primary_purpose" type="select"/>
      <bind nodeset="/data/c80_household/purpose_specify" type="string" relevant="selected( /data/c80_household/primary_purpose , 'other')"/>
      <bind nodeset="/data/c80_household/house_owner" relevant="selected( /data/c80_household/primary_purpose , 'residence')"/>
      <bind nodeset="/data/c80_household/house_owner/house_owner_given_name" type="string"/>
      <bind nodeset="/data/c80_household/house_owner/house_owner_middle_name" type="string"/>
      <bind nodeset="/data/c80_household/house_owner/house_owner_family_name" type="string"/>
      <bind nodeset="/data/c80_household/house_owner_house_name" type="string" calculate="concat( /data/c80_household/house_owner/house_owner_given_name , &quot; &quot;,  /data/c80_household/house_owner/house_owner_middle_name , &quot; &quot;,  /data/c80_household/house_owner/house_owner_family_name )"/>
      <bind nodeset="/data/c80_household/family_units" type="int" relevant="selected( /data/c80_household/primary_purpose , 'residence')" jr:constraintMsg="Must be less than or equal to 20" constraint=". &lt;= 20 and . &gt;= 1"/>
      <bind nodeset="/data/c80_household/ask_chhaupadi" type="select1"/>
      <bind nodeset="/data/c80_household/stay_menstruate" type="select1" relevant=" /data/c80_household/ask_chhaupadi  = 'yes'"/>
      <bind nodeset="/data/c80_household/woman_stay_menstruate" type="select1" relevant="( /data/c80_household/stay_menstruate  = 'shed_attached' or  /data/c80_household/stay_menstruate  = 'shed_outside') and  /data/c80_household/ask_chhaupadi  = 'yes'"/>
      <bind nodeset="/data/c80_household/observation_shed" type="select1" relevant=" /data/c80_household/ask_chhaupadi  = 'yes'"/>
      <bind nodeset="/data/c80_household/survey_complete" type="string" calculate="if( /data/c80_household/family_units  != '', 'yes', 'no')"/>
      <bind nodeset="/data/c80_household/survey_incomplete_note" readonly="true()" type="string" relevant=" /data/c80_household/survey_complete  != 'yes'"/>
      <bind nodeset="/data/c80_household/survey_complete_note" readonly="true()" type="string" relevant=" /data/c80_household/survey_complete  = 'yes'"/>
      <bind nodeset="/data/c80_household/reporting_today" type="string" calculate="date(today())"/>
      <bind nodeset="/data/c80_household/reporting_report_survey_complete" type="string" calculate="if( /data/c80_household/survey_complete  = 'yes', 1, 0)"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id">
          <label ref="jr:itext('/data/inputs/user/contact_id:label')"/>
        </input>
        <input ref="/data/inputs/user/facility_id">
          <label ref="jr:itext('/data/inputs/user/facility_id:label')"/>
        </input>
        <input ref="/data/inputs/user/name">
          <label ref="jr:itext('/data/inputs/user/name:label')"/>
        </input>
      </group>
    </group>
    <group jr:count=" /data/c80_household_count " appearance="field-list" ref="/data/c80_household">
      <input ref="/data/c80_household/generated_note_name_15">
        <label ref="jr:itext('/data/c80_household/generated_note_name_15:label')"/>
      </input>
      <input ref="/data/c80_household/house_no_suffix">
        <label ref="jr:itext('/data/c80_household/house_no_suffix:label')"/>
      </input>
      <input ref="/data/c80_household/house_no">
        <label ref="jr:itext('/data/c80_household/house_no:label')"/>
      </input>
      <select ref="/data/c80_household/primary_purpose">
        <label ref="jr:itext('/data/c80_household/primary_purpose:label')"/>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/residence:label')"/>
          <value>residence</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/business:label')"/>
          <value>business</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/government_office:label')"/>
          <value>government_office</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/hospital_health_center_clinic:label')"/>
          <value>hospital_health_center_clinic</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/religious:label')"/>
          <value>religious</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/education_institution:label')"/>
          <value>education_institution</value>
        </item>
        <item>
          <label ref="jr:itext('/data/c80_household/primary_purpose/other:label')"/>
          <value>other</value>
        </item>
      </select>
      <input ref="/data/c80_household/purpose_specify">
        <label ref="jr:itext('/data/c80_household/purpose_specify:label')"/>
      </input>
      <group appearance="field-list" ref="/data/c80_household/house_owner">
