<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>PSuPPs1</h:title>
    <model>
      <itext>
        <translation lang="en">
          <text id="/psupp_session/epds/epds_id:label">
            <value>pssupp form</value>
          </text>
          <text id="/psupp_session/epds/psupp/no:label">
            <value>No</value>
          </text>
          <text id="/psupp_session/epds/psupp/yes:label">
            <value>Yes</value>
          </text>
          <text id="/psupp_session/epds/psupp:label">
            <value>निद्रा अनुगमन चार्ट</value>
          </text>
          <text id="/psupp_session/formatted_date:label">
            <value>Formatted Date</value>
          </text>
          <text id="/psupp_session/inputs/contact/_id:label">
            <value>What is the patient's name?</value>
          </text>
          <text id="/psupp_session/inputs/contact/nepali_full_name:label">
            <value>Short Name</value>
          </text>
          <text id="/psupp_session/inputs/contact/patient_id:label">
            <value>Patient ID</value>
          </text>
          <text id="/psupp_session/inputs/contact:label">
            <value>Contact</value>
          </text>
          <text id="/psupp_session/inputs/source:label">
            <value>Source</value>
          </text>
          <text id="/psupp_session/inputs/source_id:label">
            <value>Source ID</value>
          </text>
          <text id="/psupp_session/inputs:label">
            <value>Patient</value>
          </text>
        </translation>
        <translation lang="ne">
          <text id="/psupp_session/epds/epds_id:label">
            <value>pssupp form</value>
          </text>
          <text id="/psupp_session/epds/psupp/no:label">
            <value>छैन</value>
          </text>
          <text id="/psupp_session/epds/psupp/yes:label">
            <value>छ</value>
          </text>
          <text id="/psupp_session/epds/psupp:label">
            <value>Sleep monitoring chart</value>
          </text>
          <text id="/psupp_session/formatted_date:label">
            <value>-</value>
          </text>
          <text id="/psupp_session/inputs/contact/_id:label">
            <value>सेवा लिनेको नाम के हो?</value>
          </text>
        </translation>
      </itext>
      <instance>
        <psupp_session id="psupp_session" prefix="J1!psupp_session!" delimiter="#" version="1">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <nepali_full_name/>
              <patient_id/>
              <date_of_registration/>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_name/>
          <patient_id/>
          <delivery_date tag="hidden"/>
          <updated_dd tag="hidden"/>
          <latestlmp tag="hidden"/>
          <lmpdays/>
          <ddno/>
          <up_dd/>
          <formatted_date/>
          <epds>
            <epds_id/>
            <psupp/>
          </epds>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </psupp_session>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <bind nodeset="/psupp_session/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/psupp_session/inputs/source" type="string"/>
      <bind nodeset="/psupp_session/inputs/source_id" type="string"/>
      <bind nodeset="/psupp_session/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/psupp_session/inputs/contact/nepali_full_name" type="string"/>
      <bind nodeset="/psupp_session/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/psupp_session/inputs/contact/date_of_registration" type="string"/>
      <bind nodeset="/psupp_session/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/psupp_session/patient_name" type="string" calculate="../inputs/contact/nepali_full_name"/>
      <bind nodeset="/psupp_session/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/psupp_session/delivery_date" type="string" calculate="if(instance('contact-summary')/context/delivery_date, instance('contact-summary')/context/delivery_date, .)"/>
      <bind nodeset="/psupp_session/updated_dd" type="string" calculate="if(instance('contact-summary')/context/updated_dd, instance('contact-summary')/context/updated_dd, .)"/>
      <bind nodeset="/psupp_session/latestlmp" type="string" calculate="if(instance('contact-summary')/context/latestlmp, instance('contact-summary')/context/latestlmp, .)"/>
      <bind nodeset="/psupp_session/lmpdays" type="string" calculate="round((decimal-date-time(today()) - decimal-date-time(date( /psupp_session/latestlmp ))) div 7)"/>
      <bind nodeset="/psupp_session/ddno" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /psupp_session/delivery_date ))"/>
      <bind nodeset="/psupp_session/up_dd" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /psupp_session/updated_dd ))"/>
      <bind nodeset="/psupp_session/formatted_date" type="string" calculate="format-date( /psupp_session/delivery_date , '%d/%m/%Y')"/>
      <bind nodeset="/psupp_session/epds/epds_id" type="string"/>
      <bind nodeset="/psupp_session/epds/psupp" type="select1"/>
      <bind nodeset="/psupp_session/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/psupp_session/inputs">
      <label ref="jr:itext('/psupp_session/inputs:label')"/>
      <group ref="/psupp_session/inputs/contact">
        <label ref="jr:itext('/psupp_session/inputs/contact:label')"/>
        <input appearance="db-object" ref="/psupp_session/inputs/contact/_id">
          <label ref="jr:itext('/psupp_session/inputs/contact/_id:label')"/>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/psupp_session/epds">
      <input ref="/psupp_session/epds/epds_id">
        <label ref="jr:itext('/psupp_session/epds/epds_id:label')"/>
      </input>
      <select1 ref="/psupp_session/epds/psupp">
        <label ref="jr:itext('/psupp_session/epds/psupp:label')"/>
        <item>
          <label ref="jr:itext('/psupp_session/epds/psupp/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/psupp_session/epds/psupp/no:label')"/>
          <value>no</value>
        </item>
      </select1>
    </group>
  </h:body>
</h:html>
