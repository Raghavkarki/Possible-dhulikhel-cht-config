      <bind nodeset="/stock_out/initial_condoms_phc" type="string" calculate="if(instance('contact-summary')/context/initial_condoms_phc, instance('contact-summary')/context/initial_condoms_phc, .)"/>
      <bind nodeset="/stock_out/initial_total_condoms" type="string" calculate="if(instance('contact-summary')/context/initial_total_condoms, instance('contact-summary')/context/initial_total_condoms, .)"/>
