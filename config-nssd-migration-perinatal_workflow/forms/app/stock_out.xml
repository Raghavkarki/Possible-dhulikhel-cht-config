      <bind nodeset="/stock_out/available_condom" type="string" calculate="if(instance('contact-summary')/context/available_condom, instance('contact-summary')/context/available_condom, .)"/>
      <bind nodeset="/stock_out/available_upt_kit" type="string" calculate="if(instance('contact-summary')/context/available_upt_kit, instance('contact-summary')/context/available_upt_kit, .)"/>
