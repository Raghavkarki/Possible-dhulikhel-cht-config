      <bind nodeset="/stock_out/zinc10_distritibution_u2" type="select1" required="true()" relevant="selected( /stock_out/item_out , 'zinc')"/>
      <bind nodeset="/stock_out/zinc_20mg" type="int" jr:constraintMsg="You do not have that many Zinc 20 mg" required="true()" constraint=". &lt;= int( /stock_out/available_zinc_20mg )" relevant="selected( /stock_out/item_out , 'zinc')"/>
