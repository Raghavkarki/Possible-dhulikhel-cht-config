      <bind nodeset="/family_audio/reporting_today_nepali" type="string" calculate="to-bikram-sambat( /family_audio/reporting_today )"/>
      <bind nodeset="/family_audio/reporting_nepali_short" type="string" calculate="substring-after( /family_audio/reporting_today_nepali , &quot; &quot;)"/>
      <bind nodeset="/family_audio/reporting_nepali_month" type="string" calculate="substring-before( /family_audio/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/family_audio/reporting_nepali_year" type="string" calculate="substring-after( /family_audio/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/family_audio/reporting_month_today" type="string" calculate="if( /family_audio/reporting_nepali_month  = 'बैशाख', 'Baishakh', if( /family_audio/reporting_nepali_month  = 'जेठ', 'Jestha', if( /family_audio/reporting_nepali_month  = 'असार', 'Ashadh', if( /family_audio/reporting_nepali_month  = 'साउन', 'Shrawan', if( /family_audio/reporting_nepali_month  = 'भदौ', 'Bhadra', if( /family_audio/reporting_nepali_month  = 'असोज', 'Ashwin', if( /family_audio/reporting_nepali_month  = 'कार्तिक', 'Kartik', if( /family_audio/reporting_nepali_month  = 'मंसिर', 'Mangsir', if( /family_audio/reporting_nepali_month  = 'पौष', 'Poush', if( /family_audio/reporting_nepali_month  = 'माघ', 'Magh', if( /family_audio/reporting_nepali_month  = 'फाल्गुन', 'Falgun', if( /family_audio/reporting_nepali_month  = 'चैत', 'Chaitra', ''))))))))))))"/>
      <bind nodeset="/family_audio/reporting_year_today" type="string" calculate="concat(if(substr( /family_audio/reporting_nepali_year , 0, 1) = '०', '0', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '१', '1', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '२', '2', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '३', '3', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '४', '4', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '५', '5', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '६', '6', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '७', '7', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '८', '8', if(substr( /family_audio/reporting_nepali_year , 0, 1) = '९', '9', '')))))))))), if(substr( /family_audio/reporting_nepali_year , 1, 2) = '०', '0', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '१', '1', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '२', '2', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '३', '3', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '४', '4', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '५', '5', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '६', '6', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '७', '7', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '८', '8', if(substr( /family_audio/reporting_nepali_year , 1, 2) = '९', '9', '')))))))))), if(substr( /family_audio/reporting_nepali_year , 2, 3) = '०', '0', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '१', '1', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '२', '2', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '३', '3', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '४', '4', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '५', '5', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '६', '6', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '७', '7', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '८', '8', if(substr( /family_audio/reporting_nepali_year , 2, 3) = '९', '9', '')))))))))), if(substr( /family_audio/reporting_nepali_year , 3, 4) = '०', '0', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '१', '1', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '२', '2', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '३', '3', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '४', '4', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '५', '5', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '६', '6', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '७', '7', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '८', '8', if(substr( /family_audio/reporting_nepali_year , 3, 4) = '९', '9', '')))))))))))"/>
      <bind nodeset="/family_audio/reporting_today_short" type="string" calculate="concat( /family_audio/reporting_month_today , ' ',  /family_audio/reporting_year_today )"/>
