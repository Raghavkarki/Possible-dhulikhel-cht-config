      <bind nodeset="/psupp_home_visit/lmpdays" type="string" calculate="round((decimal-date-time(today()) - decimal-date-time(date( /psupp_home_visit/latestlmp ))) div 7)"/>
      <bind nodeset="/psupp_home_visit/ddno" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /psupp_home_visit/delivery_date ))"/>
      <bind nodeset="/psupp_home_visit/up_dd" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /psupp_home_visit/updated_dd ))"/>
      <bind nodeset="/psupp_home_visit/formatted_date" type="string" calculate="format-date( /psupp_home_visit/delivery_date , '%d/%m/%Y')"/>
      <bind nodeset="/psupp_home_visit/epds/epds_id" type="string"/>
      <bind nodeset="/psupp_home_visit/epds/psupp" type="select1"/>
