      <bind nodeset="/death_and_migration_form/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/death_and_migration_form/inputs/source" type="string"/>
      <bind nodeset="/death_and_migration_form/inputs/source_id" type="string"/>
      <bind nodeset="/death_and_migration_form/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/death_and_migration_form/inputs/contact/nepali_full_name" type="string"/>
      <bind nodeset="/death_and_migration_form/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/death_and_migration_form/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/death_and_migration_form/patient_name" type="string" calculate="../inputs/contact/nepali_full_name"/>
      <bind nodeset="/death_and_migration_form/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/death_and_migration_form/reporting_today" type="string" calculate="date(today())"/>
      <bind nodeset="/death_and_migration_form/reporting_today_nepali" type="string" calculate="to-bikram-sambat( /death_and_migration_form/reporting_today )"/>
      <bind nodeset="/death_and_migration_form/reporting_nepali_short" type="string" calculate="substring-after( /death_and_migration_form/reporting_today_nepali , &quot; &quot;)"/>
      <bind nodeset="/death_and_migration_form/reporting_nepali_month" type="string" calculate="substring-before( /death_and_migration_form/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/death_and_migration_form/reporting_nepali_year" type="string" calculate="substring-after( /death_and_migration_form/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/death_and_migration_form/reporting_month_today" type="string" calculate="if( /death_and_migration_form/reporting_nepali_month  = 'बैशाख', 'Baishakh', if( /death_and_migration_form/reporting_nepali_month  = 'जेठ', 'Jestha', if( /death_and_migration_form/reporting_nepali_month  = 'असार', 'Ashadh', if( /death_and_migration_form/reporting_nepali_month  = 'साउन', 'Shrawan', if( /death_and_migration_form/reporting_nepali_month  = 'भदौ', 'Bhadra', if( /death_and_migration_form/reporting_nepali_month  = 'असोज', 'Ashwin', if( /death_and_migration_form/reporting_nepali_month  = 'कार्तिक', 'Kartik', if( /death_and_migration_form/reporting_nepali_month  = 'मंसिर', 'Mangsir', if( /death_and_migration_form/reporting_nepali_month  = 'पौष', 'Poush', if( /death_and_migration_form/reporting_nepali_month  = 'माघ', 'Magh', if( /death_and_migration_form/reporting_nepali_month  = 'फाल्गुन', 'Falgun', if( /death_and_migration_form/reporting_nepali_month  = 'चैत', 'Chaitra', ''))))))))))))"/>
      <bind nodeset="/death_and_migration_form/reporting_year_today" type="string" calculate="concat(if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '०', '0', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '१', '1', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '२', '2', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '३', '3', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '४', '4', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '५', '5', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '६', '6', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '७', '7', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '८', '8', if(substr( /death_and_migration_form/reporting_nepali_year , 0, 1) = '९', '9', '')))))))))), if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '०', '0', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '१', '1', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '२', '2', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '३', '3', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '४', '4', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '५', '5', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '६', '6', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '७', '7', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '८', '8', if(substr( /death_and_migration_form/reporting_nepali_year , 1, 2) = '९', '9', '')))))))))), if(substr( /death_and_migration_form/reporting_nepali_year , 2, 3) = '०', '0', if(substr( /death_and_migration_form/reporting_nepali_year , 2, 3) = '१', '1', if(substr( /death_and_migration_form/reporting_nepali_year , 2, 3) = '२', '2', if(substr( /death_and_migration_form/reporting_nepali_