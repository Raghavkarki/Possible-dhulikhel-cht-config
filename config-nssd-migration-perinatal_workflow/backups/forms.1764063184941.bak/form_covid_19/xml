      <bind nodeset="/covid_19/reporting_date_nepali" type="string" calculate="to-bikram-sambat( /covid_19/reporting_date )"/>
      <bind nodeset="/covid_19/reporting_nepali_short" type="string" calculate="substring-after( /covid_19/reporting_date_nepali , &quot; &quot;)"/>
      <bind nodeset="/covid_19/reporting_nepali_month" type="string" calculate="substring-before( /covid_19/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/covid_19/reporting_nepali_year" type="string" calculate="substring-after( /covid_19/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/covid_19/reporting_month_today" type="string" calculate="if( /covid_19/reporting_nepali_month  = 'बैशाख', 'Baishakh', if( /covid_19/reporting_nepali_month  = 'जेठ', 'Jestha', if( /covid_19/reporting_nepali_month  = 'असार', 'Ashadh', if( /covid_19/reporting_nepali_month  = 'साउन', 'Shrawan', if( /covid_19/reporting_nepali_month  = 'भदौ', 'Bhadra', if( /covid_19/reporting_nepali_month  = 'असोज', 'Ashwin', if( /covid_19/reporting_nepali_month  = 'कार्तिक', 'Kartik', if( /covid_19/reporting_nepali_month  = 'मंसिर', 'Mangsir', if( /covid_19/reporting_nepali_month  = 'पौष', 'Poush', if( /covid_19/reporting_nepali_month  = 'माघ', 'Magh', if( /covid_19/reporting_nepali_month  = 'फाल्गुन', 'Falgun', if( /covid_19/reporting_nepali_month  = 'चैत', 'Chaitra', ''))))))))))))"/>
      <bind nodeset="/covid_19/reporting_year_today" type="string" calculate="concat(if(substr( /covid_19/reporting_nepali_year , 0, 1) = '०', '0', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '१', '1', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '२', '2', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '३', '3', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '४', '4', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '५', '5', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '६', '6', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '७', '7', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '८', '8', if(substr( /covid_19/reporting_nepali_year , 0, 1) = '९', '9', '')))))))))), if(substr( /covid_19/reporting_nepali_year , 1, 2) = '०', '0', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '१', '1', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '२', '2', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '३', '3', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '४', '4', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '५', '5', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '६', '6', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '७', '7', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '८', '8', if(substr( /covid_19/reporting_nepali_year , 1, 2) = '९', '9', '')))))))))), if(substr( /covid_19/reporting_nepali_year , 2, 3) = '०', '0', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '१', '1', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '२', '2', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '३', '3', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '४', '4', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '५', '5', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '६', '6', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '७', '7', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '८', '8', if(substr( /covid_19/reporting_nepali_year , 2, 3) = '९', '9', '')))))))))), if(substr( /covid_19/reporting_nepali_year , 3, 4) = '०', '0', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '१', '1', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '२', '2', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '३', '3', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '४', '4', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '५', '5', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '६', '6', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '७', '7', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '८', '8', if(substr( /covid_19/reporting_nepali_year , 3, 4) = '९', '9', '')))))))))))"/>
