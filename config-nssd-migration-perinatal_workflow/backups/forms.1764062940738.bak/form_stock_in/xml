      <bind nodeset="/stock_in/inputs" required="field-list"/>
      <bind nodeset="/stock_in/inputs/source" type="string"/>
      <bind nodeset="/stock_in/inputs/source_id" type="string"/>
      <bind nodeset="/stock_in/inputs/contact/_id" type="db:person" required="db-object"/>
      <bind nodeset="/stock_in/inputs/contact/name" type="string"/>
      <bind nodeset="/stock_in/chw_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/stock_in/chw_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/stock_in/chw_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/stock_in/date_of_entry" type="string" calculate="date(today())"/>
      <bind nodeset="/stock_in/date_of_entry_nepali" type="string" calculate="to-bikram-sambat( /stock_in/date_of_entry )"/>
      <bind nodeset="/stock_in/reporting_nepali_short" type="string" calculate="substring-after( /stock_in/date_of_entry_nepali , &quot; &quot;)"/>
      <bind nodeset="/stock_in/reporting_nepali_month" type="string" calculate="substring-before( /stock_in/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/stock_in/reporting_nepali_year" type="string" calculate="substring-after( /stock_in/reporting_nepali_short , &quot; &quot;)"/>
      <bind nodeset="/stock_in/reporting_month_today" type="string" calculate="if( /stock_in/reporting_nepali_month  = 'बैशाख', 'Baishakh', if( /stock_in/reporting_nepali_month  = 'जेठ', 'Jestha', if( /stock_in/reporting_nepali_month  = 'असार', 'Ashadh', if( /stock_in/reporting_nepali_month  = 'साउन', 'Shrawan', if( /stock_in/reporting_nepali_month  = 'भदौ', 'Bhadra', if( /stock_in/reporting_nepali_month  = 'असोज', 'Ashwin', if( /stock_in/reporting_nepali_month  = 'कार्तिक', 'Kartik', if( /stock_in/reporting_nepali_month  = 'मंसिर', 'Mangsir', if( /stock_in/reporting_nepali_month  = 'पौष', 'Poush', if( /stock_in/reporting_nepali_month  = 'माघ', 'Magh', if( /stock_in/reporting_nepali_month  = 'फाल्गुन', 'Falgun', if( /stock_in/reporting_nepali_month  = 'चैत', 'Chaitra', ''))))))))))))"/>
      <bind nodeset="/stock_in/reporting_year_today" type="string" calculate="concat(if(substr( /stock_in/reporting_nepali_year , 0, 1) = '०', '0', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '१', '1', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '२', '2', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '३', '3', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '४', '4', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '५', '5', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '६', '6', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '७', '7', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '८', '8', if(substr( /stock_in/reporting_nepali_year , 0, 1) = '९', '9', '')))))))))), if(substr( /stock_in/reporting_nepali_year , 1, 2) = '०', '0', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '१', '1', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '२', '2', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '३', '3', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '४', '4', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '५', '5', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '६', '6', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '७', '7', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '८', '8', if(substr( /stock_in/reporting_nepali_year , 1, 2) = '९', '9', '')))))))))), if(substr( /stock_in/reporting_nepali_year , 2, 3) = '०', '0', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '१', '1', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '२', '2', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '३', '3', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '४', '4', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '५', '5', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '६', '6', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '७', '7', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '८', '8', if(substr( /stock_in/reporting_nepali_year , 2, 3) = '९', '9', '')))))))))), if(substr( /stock_in/reporting_nepali_year , 3, 4) = '०', '0', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '१', '1', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '२', '2', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '३', '3', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '४', '4', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '५', '5', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '६', '6', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '७', '7', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '८', '8', if(substr( /stock_in/reporting_nepali_year , 3, 4) = '९', '9', '')))))))))))"/>
      <bind nodeset="/stock_in/reporting_today_short" type="string" calculate="concat( /stock_in/reporting_month_today , ' ',  /stock_in/reporting_year_today )"/>
      <bind nodeset="/stock_in/items_selected" type="select" required="true()"/>
      <bind nodeset="/stock_in/ors_received" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'ors')"/>
      <bind nodeset="/stock_in/zinc_10_mg" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'zinc')"/>
      <bind nodeset="/stock_in/zinc_20_mg" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'zinc')"/>
      <bind nodeset="/stock_in/upt_received" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'upt_kit')"/>
      <bind nodeset="/stock_in/condom_hp" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'condom')"/>
      <bind nodeset="/stock_in/condom_phcc" type="int" required="true()" relevant="selected( /stock_in/items_selected , 'condom')"/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_ors" type="string" calculate=" /stock_in/ors_received "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_zinc10mg" type="string" calculate=" /stock_in/zinc_10_mg "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_zinc20mg" type="string" calculate=" /stock_in/zinc_20_mg "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_UPT_kits" type="string" calculate=" /stock_in/upt_received "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_condoms_hp" type="string" calculate=" /stock_in/condom_hp "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_condoms_phc" type="string" calculate=" /stock_in/condom_phcc "/>
      <bind nodeset="/stock_in/initial_stock/initial_stock_total_condoms" type="string" calculate="number( /stock_in/condom_hp ) + number( /stock_in/condom_phcc )"/>
      <bind nodeset="/stock_in/initial_stock/condom_total" type="string" calculate="number( /stock_in/condom_hp ) + number( /stock_in/condom_phcc )"/>
      <bind nodeset="/stock_in/initial_stock/total_number" readonly="true()" type="string"/>
      <bind nodeset="/stock_in/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
