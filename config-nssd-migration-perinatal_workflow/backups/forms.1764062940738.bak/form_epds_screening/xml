      <bind nodeset="/epds_screening/patient_name" type="string" calculate="../inputs/contact/nepali_full_name"/>
      <bind nodeset="/epds_screening/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/epds_screening/delivery_date" type="string" calculate="if(instance('contact-summary')/context/delivery_date, instance('contact-summary')/context/delivery_date, .)"/>
      <bind nodeset="/epds_screening/updated_dd" type="string" calculate="if(instance('contact-summary')/context/updated_dd, instance('contact-summary')/context/updated_dd, .)"/>
      <bind nodeset="/epds_screening/latestlmp" type="string" calculate="if(instance('contact-summary')/context/latestlmp, instance('contact-summary')/context/latestlmp, .)"/>
      <bind nodeset="/epds_screening/lmpdays" type="string" calculate="round((decimal-date-time(today()) - decimal-date-time(date( /epds_screening/latestlmp ))) div 7)"/>
