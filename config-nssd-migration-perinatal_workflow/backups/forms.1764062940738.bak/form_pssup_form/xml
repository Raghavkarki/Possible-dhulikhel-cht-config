          <text id="/pssup_form/inputs/contact/patient_id:label">
            <value>Patient ID</value>
          </text>
          <text id="/pssup_form/inputs/contact:label">
            <value>Contact</value>
          </text>
          <text id="/pssup_form/inputs/source:label">
            <value>Source</value>
          </text>
          <text id="/pssup_form/inputs/source_id:label">
            <value>Source ID</value>
          </text>
          <text id="/pssup_form/inputs:label">
            <value>Patient</value>
          </text>
        </translation>
        <translation lang="ne">
          <text id="/pssup_form/epds/epds_id:label">
            <value>pssupp form</value>
          </text>
          <text id="/pssup_form/formatted_date:label">
            <value>-</value>
          </text>
          <text id="/pssup_form/inputs/contact/_id:label">
            <value>सेवा लिनेको नाम के हो?</value>
          </text>
        </translation>
      </itext>
      <instance>
        <pssup_form id="psupp_form" prefix="J1!psupp_form!" delimiter="#" version="1">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <nepali_full_name/>
              <patient_id/>
              <date_of_registration/>
            </contact>
          </inputs>
          <patient_uuid/>
          <patient_name/>
          <patient_id/>
          <delivery_date tag="hidden"/>
          <updated_dd tag="hidden"/>
          <latestlmp tag="hidden"/>
          <lmpdays/>
          <ddno/>
          <up_dd/>
          <formatted_date/>
          <epds>
            <epds_id/>
          </epds>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </pssup_form>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <bind nodeset="/pssup_form/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pssup_form/inputs/source" type="string"/>
      <bind nodeset="/pssup_form/inputs/source_id" type="string"/>
      <bind nodeset="/pssup_form/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/pssup_form/inputs/contact/nepali_full_name" type="string"/>
      <bind nodeset="/pssup_form/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/pssup_form/inputs/contact/date_of_registration" type="string"/>
      <bind nodeset="/pssup_form/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pssup_form/patient_name" type="string" calculate="../inputs/contact/nepali_full_name"/>
      <bind nodeset="/pssup_form/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pssup_form/delivery_date" type="string" calculate="if(instance('contact-summary')/context/delivery_date, instance('contact-summary')/context/delivery_date, .)"/>
      <bind nodeset="/pssup_form/updated_dd" type="string" calculate="if(instance('contact-summary')/context/updated_dd, instance('contact-summary')/context/updated_dd, .)"/>
      <bind nodeset="/pssup_form/latestlmp" type="string" calculate="if(instance('contact-summary')/context/latestlmp, instance('contact-summary')/context/latestlmp, .)"/>
      <bind nodeset="/pssup_form/lmpdays" type="string" calculate="round((decimal-date-time(today()) - decimal-date-time(date( /pssup_form/latestlmp ))) div 7)"/>
      <bind nodeset="/pssup_form/ddno" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /pssup_form/delivery_date ))"/>
      <bind nodeset="/pssup_form/up_dd" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /pssup_form/updated_dd ))"/>
      <bind nodeset="/pssup_form/formatted_date" type="string" calculate="format-date( /pssup_form/delivery_date , '%d/%m/%Y')"/>
      <bind nodeset="/pssup_form/epds/epds_id" type="string"/>
      <bind nodeset="/pssup_form/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
